<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="192dp"
    android:height="192dp"
    android:viewportWidth="192"
    android:viewportHeight="192">
    <path
        android:name="background"
        android:fillColor="?attr/play_button_pause_background"
        android:pathData="@string/drawable_circular" />
    <group
        android:name="rotationGroup"
        android:pivotX="96"
        android:pivotY="96">
        <path
            android:name="left"
            android:fillColor="?attr/play_button_icon_tint"
            android:pathData="@string/drawable_vector_pause_left" />
        <path
            android:name="right"
            android:fillColor="?attr/play_button_icon_tint"
            android:pathData="@string/drawable_vector_pause_right" />
    </group>
</vector>